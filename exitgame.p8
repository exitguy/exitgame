function spawn_enemies()
 local campos=((player.x-40)/8)
 for x=campos,campos+15 do
  for y=(level/8),(level/8)+15 do
   if level==0 or level==127 then
     eway=-.2
     eflip=false
   else
     eway=.2
     eflip=true
   end
   -- robots
   if mget(x,y)==20 then
    add(enemies,{
     x=x*8,
     y=y*8,
     w=7,
     h=7,
     f=eflip,
     sp=21,
     sf=21,
     sc=2,
     st=0,
     way=eway
    })
    mset(x,y,0)
   end -- robot
  end
 end
end

function replace_tiles()
 for x=0,127 do
   for y=0,63 do
     -- robots
     if mget(x,y)==16 then
       mset(x,y,17)
     end
   end
 end
end

-- check overlap, shamelessly copied from: 
-- https://mboffin.itch.io/pico8-overlap
function overlap(a,b)
 return not (a.x>b.x+b.w 
          or a.y>b.y+b.h 
          or a.x+a.w<b.x 
          or a.y+a.h<b.y)
end

-- collision detection --
function ecollide(o)

 -- all corners of our enemy
 local x1=o.x/8     -- left on the x-axis
 local y1=o.y/8     -- top on the y-axis
 local y2=(o.y+7)/8 -- bottom on the y-axis
 local x2=(o.x+7)/8 -- right on the x-axis
 
 local a=fget(mget(x1,y1),1) -- top left
 local b=fget(mget(x1,y2),1) -- bottom left
 local c=fget(mget(x2,y2),1) -- bottom right
 local d=fget(mget(x2,y1),1) -- top right

 if t == "top" then
   return e
 elseif (a and b) or (c and d) then
  return true
 else
  return false
 end
end
