-- the ex-it game!
-- a dumb game for ex-it guy's website
-- 
-- written by ex-it guy
-- music by quantum exile
--
-- copyleft 2024

function _init()
 player={
  x=70,    -- x-axis
  y=112,   -- y-axis
  f=false, -- flip
  h=100,   -- health
  p=0      -- points
 }
 exitguy=1
 stimer=0
 gravity=2
 jforce=0
 state="play"
 music(0)
end

function _update()
  if (state=="gameover" or state=="theend") and btn(‚ùé) then
    state="play"
    _init()
    reload(0x1000, 0x1000, 0x2000)
  end 
  dground(player)
  move(player)
  jump(player)
  pickups(player)
  if player.h<=0 then
    state="gameover"
  end
  if btnp(üÖæÔ∏è) then
    music(-1)
  end
end

function _draw()
 if state=="play" then
  cls(1)
  map()
  spr(exitguy,player.x,player.y,1,1,player.f)
  camera((player.x - 40),0)
  print("health: "..player.h,player.x - 30, 10)
  print("points: "..player.p,player.x - 30, 16)
 elseif state=="gameover" then
  cls(0)
  camera(0,0)
  print("game over!",32,32)
  print("your score: "..player.p,32,40)
  print("press ‚ùé to restart",32,64)
 elseif state=="theend" then
  cls(2)
  camera(0,0)
  print("you've reached the exit!",32,32)
  print("your score: "..player.p,32,40)
  print("press ‚ùé to play again!",32,64)
 end
end
