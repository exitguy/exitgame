__lua__
-- the ex-it game!
-- a dumb game for ex-it guy's website
-- 
-- written by ex-it guy
-- music by quantum exile
--

function _init()
 starttime=time()
 player={
  x=70,    -- x-axis
  y=112,   -- y-axis
  w=7,
  h=7,
  f=false, -- flip
  health=100,   -- health
  p=0,     -- points
  sf=1,    -- first sprite
  sp=1,    -- current sprite
  sc=3,    -- sprite count
  st=0     -- sprite timer
 }
 enemies={}
 dtimer=0
 gravity=2
 jforce=0
 state="play"
 level=0
 prevlevel=0
 music(0)
 replace_tiles()
end

function _update()
  if (state=="gameover" or state=="theend") and btn(â) then
    state="play"
    reload(0x1000, 0x1000, 0x2000)
    _init()
  end 
  if state=="gameover" then
    if player.health<=0 then
      music(9)
      player.health=100
    end
  elseif state=="lvlup" then
    if level - prevlevel < 128 then
      level += 2
      player.y += 2
    end
    if level - prevlevel == 128 then
      prevlevel+=128
      state="play"
    end
  elseif state=="play" then
    move(player)
    jump(player)
    pickups(player)
    if player.health<=0 then
      die(player)
      sfx(-1)
    end
    if btnp(ğŸ…¾ï¸) then
      music(-1)
    end
    spawn_enemies()
    for e in all(enemies) do
     e.x+=e.way
     if collide(e) or ecollide(e) then
      if e.way==.2 then
        e.way=-.2
        e.f=false
      else
        e.way=.2
        e.f=true
      end
      e.x+=e.way
     end
     grav(e)
     animate(e)
     if overlap(e,player) then
       player.health-=20
       if player.x<e.x then
         player.x-=8
       else
         player.x+=8
       end
       jforce=10
       sfx(3)
     end
    end
  end
end

function _draw()
 if state=="play" or state=="lvlup" then
  cls(1)
  spr(player.sp,player.x,player.y,1,1,player.f)
  map()
  camera((player.x - 40),0+level)
  dsprint("health: "..player.health,player.x -32, level +2 ,11,0)
  dsprint("score: "..player.p,player.x +30, level +2 ,11,0)  
  dsprint("temp. exit",108*8, 220,11,0)
  dsprint("level not done yet ;-)",108*8, 226,11,0)
  for e in all(enemies) do
   spr(e.sp,e.x,e.y,1,1,e.f)
  end 
 elseif state=="gameover" then
  cls(0)
  camera(0,0)
  sspr(104, 24, 8, 8, 16, 8, 96, 96)
  print("game over!",45,0,15)
  dsprint("your score", 45,60,15,0)
  local scorelen=#tostring(player.p)
  dsprint(player.p,64-(scorelen*2),70,15,0)
  print("press â to restart",28,108)
  print("https://youtube.com/@ex-itguy",11,121,15)
  spr(62,1,120,1,1)
 elseif state=="theend" then
  cls(3)
  camera(0,0)
  sspr(8, 0, 8, 8, 16, 16, 96, 96)
  dsprint("you've reached the exit!",20,32,0,11)
  dsprint("your score: "..player.p,20,40,0,11)
  dsprint("press â to play again!",20,64,0,11)
  dsprint("time played: "..timeplayed,20,110,0,11)
  dsprint("https://youtube.com/@ex-itguy",11,121,15,0)
  spr(62,1,120,1,1)
 end
end
