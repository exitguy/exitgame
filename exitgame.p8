function die(o)
  state="gameover"
  sfx(3)
end

-- collision detection --
function collide(o)

 -- find all corners of our player
 local x1=o.x/8     -- left on the x-axis
 local y1=o.y/8     -- top on the y-axis
 local y2=(o.y+7)/8 -- bottom on the y-axis
 local x2=(o.x+7)/8 -- right on the x-axis
 local x3=(o.x+3)/8 -- center on the x-axis
 
 local a=fget(mget(x1,y1),0) -- top left
 local b=fget(mget(x1,y2),0) -- bottom left
 local c=fget(mget(x2,y2),0) -- bottom right
 local d=fget(mget(x2,y1),0) -- top right
 local e=fget(mget(x3,y1),0) -- top center

 if t == "top" then
   return e
 elseif a or b or c or d then
  return true
 else
  return false
 end
end

-- is our player on a surface with flag 0?
function onground(o)
 local a = fget(mget((o.x)/8,(o.y+8)/8),0)
 local b = fget(mget((o.x+7)/8,(o.y+8)/8),0)
 if a or b then
  return true
 else
  return false
 end
end
function nearground(o)
 local a = fget(mget((o.x)/8,(o.y+9)/8),0)
 local b = fget(mget((o.x+7)/8,(o.y+9)/8),0)
 if a or b then
  return true
 else
  return false
 end
end

function animate(o)
  if o.st<4 then
   o.st+=1
  else
   if o.sp<(o.sf+o.sc-1) then
    o.sp+=1
   else
    o.sp=o.sf
   end
   o.st=0
  end
end

-- gravity
function grav(o)
 if onground(o) then
   gravity=2
 elseif nearground(o) then
   gravity=1
 else
   gravity=2
 end
 if not onground(o) then
   o.y+=gravity
 end
end

function move(o) --move an object
 local lx=o.x --last x pos
-- local ly=o.y --last y pos
  
 if (btn(â¬…ï¸)) then
   if o.x >=0 then
     o.x-=2
   end 
   animate(o)
   o.f=true
 end
 if (btn(â¡ï¸)) then
   o.x+=2
   animate(o)
   o.f=false
 end

 -- if it collides, move back
 if collide(o) then
  o.x=lx
--  o.y=ly
 end 
end

function jump(o)
 local ly=o.y --last y pos
 
  --jumping--
 if btnp(â) or btnp(â¬†ï¸) then
  if onground(o) then
   jforce=21
   --sfx(0)
  end
 end
 
 if jforce>0 then
  jforce=jforce*0.6
 end
 
 o.y-=jforce
 
 --gravity--
 grav(o)
  
 -- if it collides, move back
 if collide(o,"top") then
   o.y=ly
 end
 
 if o.y>=level+128 then
   o.health=0
 end 
end

function pickups(o) 
 local tilex=(o.x+4)/8
 local tiley=(o.y+4)/8
 local tile = mget(tilex,tiley)

 -- floppy
 if tile == 9 then
   o.p+=14
   sfx(1)
   mset(tilex,tiley,0)
 end
 -- usb stick
 if tile == 10 then
   o.p+=100
   sfx(1)
   mset(tilex,tiley,0)
 end
 -- heart
 if tile == 11 and o.health <=99 then
   if o.health<=90 then
     o.health+=10
   elseif o.health<=99 then
     o.health=100
   end
   sfx(2)
   mset(tilex,tiley,0)
 end
 -- printer
 if tile == 12 then
   o.health-=15
   jforce=15
   sfx(3)
 end
 -- lego
 if tile == 13 then
   o.health-=10
   jforce=15
   sfx(3)
 end
 if tile == 60 then
   levelup()
 end
 if tile == 63 then
   theend()
 end
end
